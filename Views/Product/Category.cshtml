@using Northwind.Models
@model IEnumerable<Category>


@foreach (Category category in Model)
{
<div class="list-group">
  <a asp-controller="Product" asp-action="Index" asp-route-id="@category.CategoryId" class="col-md-4 list-group-item list-group-item-info">
     @category.CategoryName &#8212
        @category.Description
  </a>
</div>
}